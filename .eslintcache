cialties.some((tag) => tag.uid === uid);\n        }\n        if (key === \"program\") {\n          return state.tags.tags?.programs.some((tag) => tag.uid === uid);\n        }\n      })\n    : false;\n\n  return (\n    <div className={block()}>\n      {!hasTag ? (\n        <div\n          className={block(\"button\")}\n          onClick={async () => {\n            await actions.tags.addTag({\n              relationId: uid,\n              relationType: key as TagRelationType,\n            });\n            await actions.tags.getMyTags();\n          }}\n        >\n          + Добавить к тегам\n        </div>\n      ) : (\n        <div\n          className={block(\"button\", { red: true })}\n          onClick={async () => {\n            await actions.tags.removeTag({\n              relationId: uid,\n              relationType: key as TagRelationType,\n            });\n            await actions.tags.getMyTags();\n          }}\n        >\n          Убрать из тегов\n        </div>\n      )}\n      <span>{hasTag ? \"Добавлено\" : \"Не добавлено\"}</span>\n      <div className={block(\"summary\")}>\n        <div className={block(\"title\")}>{title}</div>\n        <div className={block(\"info\")}>\n          <div className={block(\"type\", { ...(key && { [key]: true }) })}>\n            {type}\n          </div>\n          <div className={block(\"relations\")}>{relations}</div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nEducationSummary.type = EducationType;\nexport const EducationSummarySmart = EducationSummary;\n","/Users/kotofenum/Desktop/workspace/dstu-web-app/src/types/EducationType.ts",[],"/Users/kotofenum/Desktop/workspace/dstu-web-app/src/components/EducationRelation/index.ts",[],"/Users/kotofenum/Desktop/workspace/dstu-web-app/src/components/EducationRelation/EducationRelation.tsx",[],"/Users/kotofenum/Desktop/workspace/dstu-web-app/src/components/EducationProperties/index.ts",[],"/Users/kotofenum/Desktop/workspace/dstu-web-app/src/components/EducationProper